{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Linux) Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/app.out",
      "args": ["<", "${workspaceFolder}/input/example1.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Debug",
      "miDebuggerPath": "/usr/bin/gdb"
    },
  ]
}